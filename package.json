{
  "name": "git-copy",
  "version": "1.0.0",
  "description": "Hard copy the whole or a certain part of git repository to destination",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "preferGlobal": true,
  "scripts": {
    "build": "tsc",
    "doc": "typedoc --out docs ./src",
    "postversion": "npm run doc",
    "prepare": "npm run build",
    "test": "jest --config jest.config.json",
    "test:coverage": "jest --config jest.config.json --coverage",
    "test:coveralls": "jest --config jest.config.json --coverage --coverageReporters=text-lcov | coveralls"
  },
  "bin": {
    "git-copy": "lib/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/minnam/paak.git"
  },
  "keywords": [
    "Clone",
    "Copy Git Repo",
    "Folder",
    "Git Repo to folder",
    "Git"
  ],
  "author": "Min Nam",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/minnam/paak/issues"
  },
  "homepage": "https://github.com/minnam/paak#readme",
  "devDependencies": {
    "@olavoparno/jest-badges-readme": "^1.3.4",
    "@types/jest": "^24.0.11",
    "coveralls": "^3.0.3",
    "jest": "^24.7.1",
    "prettier": "^1.17.0",
    "ts-jest": "^24.0.2",
    "ts-loader": "^5.3.3",
    "tslint": "^5.16.0",
    "tslint-config-prettier": "^1.18.0",
    "typedoc": "^0.15.0",
    "typescript": "^3.4.4",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.0"
  },
  "dependencies": {
    "@types/node": "^12.12.4",
    "git-clone": "^0.1.0"
  }
}
